<% provide(:title, @user.name) %>
<div class="row">
  <%= render "shared/users_info" %>

  <div class="col-md-8">
    <% if @user.microposts.any? %>
      <ol class="microposts">
        <%= render @microposts %>
      </ol>
      <%= will_paginate @microposts %>
    <% end %>
  </div>
</div>
<%= debug(params) if Rails.env.development? %>
